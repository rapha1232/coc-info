{
  "name": "coc-max-levels",
  "version": "1.1.0",
  "description": "A utility package that provides maximum levels for Clash of Clans buildings, heroes, troops, and spells",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run test",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "clash-of-clans",
    "coc",
    "game",
    "levels",
    "max-levels",
    "buildings",
    "heroes",
    "troops"
  ],
  "author": "Raphael Semaan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapha1232/coc-max-levels.git"
  },
  "bugs": {
    "url": "https://github.com/rapha1232/coc-max-levels/issues"
  },
  "homepage": "https://github.com/rapha1232/coc-max-levels#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "eslint": "^9.28.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
